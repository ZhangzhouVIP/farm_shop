<view>
  <search id="search">
  </search>
</view>
<view class="swiper-container">
  <swiper indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{inteval}}" duration="{{duration}}"
  circular="{{circular}}" current="{{swiperCurrent}}" bindchange="swiperChange" class="swiper">
  <block wx:for="{{imgUrls}}" wx:key="unique">
    <swiper-item >
      <image src="{{item}}" class="img" bindtap="swipclick"/>
    </swiper-item>
  </block>
  </swiper>
</view>